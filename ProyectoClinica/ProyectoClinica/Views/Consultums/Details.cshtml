@model ProyectoClinica.Models.Consultum

@{
    ViewData["Title"] = "Detalles - Consulta";
}

<h1>Detalles</h1>

<div>
    <h4>Consulta</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Fecha de consulta
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaDeConsulta)
        </dd>
        <dt class = "col-sm-2">
            Datos subjetivos
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DatosSubjetivos)
        </dd>
        <dt class = "col-sm-2">
            Datos objetivos
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DatosObjetivos)
        </dd>
        <dt class = "col-sm-2">
            Plan terapéutico
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PlanTerapuetico)
        </dd>
        <dt class = "col-sm-2">
            Datos nuevos
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NuevosDatos)
        </dd>
        <dt class = "col-sm-2">
            Estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
        <dt class = "col-sm-2">
            Número de caso
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdcasoNavigation.Id)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Regresar a la lista</a>
</div>
