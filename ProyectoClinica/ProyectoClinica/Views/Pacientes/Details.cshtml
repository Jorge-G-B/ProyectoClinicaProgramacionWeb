@model ProyectoClinica.Models.Paciente

@{
    ViewData["Title"] = "Detalles - Paciente";
}

<h1>Detalles</h1>

<div>
    <h4>Paciente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Primer nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Pnombre)
        </dd>
        <dt class = "col-sm-2">
            Segundo nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Snombre)
        </dd>
        <dt class = "col-sm-2">
            Primer Apellido
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Papellido)
        </dd>
        <dt class = "col-sm-2">
            Segundo Apellido
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sapellido)
        </dd>
        <dt class = "col-sm-2">
            Edad
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Edad)
        </dd>
        <dt class = "col-sm-2">
            Teléfono
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class = "col-sm-2">
            Fecha de nacimiento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaDeNacimiento)
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            Sexo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sexo)
        </dd>
        <dt class = "col-sm-2">
            Nombre del responsable
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreResponsable)
        </dd>
        <dt class = "col-sm-2">
            Teléfono del responsable
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TelResponsable)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Regresar a la lista</a>
</div>
